{
  "name": "@ariran/neuro-docs",
  "version": "1.9.2",
  "main": "./lib/index.mts",
  "bin": {
    "neuro-docs": "./lib/index.mts",
    "neuro-tests": "./packages/test-interface-deep2/cli.ts"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start": "tsx --env-file=.env ./lib/index.mts",
    "tests": "tsx --env-file=.env packages/test-interface/index.mts generate '**/*.mts' -r --ref qwe --ref asdfdas -i node_modules  -l typescript -f vitest",
    "tests:go": "tsx --env-file=.env packages/test-interface/index.mts generate 'example-master/{appengine-hello,hello}/**/*.go' -r -l golang -f gotest -i *_test.go",
    "tests:go2": "tsx --env-file=.env packages/test-interface/index.mts generate 'glauth-master/v2/pkg/handler/ldap.go' -l golang -f testify --out-ext _test.go --ref 'glauth-master/v2/internal/monitoring/**/*' --ref 'glauth-master/v2/pkg/**/*' -m o1",
    "tests:console": "tsx --env-file=.env packages/test-interface/index.mts generate 'consoleFull/packages/ui/components/*/index.ts' --ref 'consoleFull/packages/ui/components/**/*' -r -l vue-typescript -f vitest --out-ext .test.ts -o consoleFull/packages/ui/tests --comment 'делай правильные импорты' --base 'consoleFull/packages/ui/' -m 'gpt-4o' ",
    "tests:console-deep2": "tsx --env-file=.env packages/test-interface-deep2/cli.ts generate 'consoleFull/packages/ui/components/*/index.ts' -l vue-typescript -f vitest -m 'deepseek/deepseek-chat' --ref 'consoleFull/packages/ui/components/**/*'",
    "tests:console-deep2:go": "tsx --env-file=.env packages/test-interface-deep2/cli.ts generate 'glauth-master/v2/pkg/handler/ldap.go' -l golang -f testify -m 'deepseek/deepseek-chat'",
    "tests:console-deep2:go-g:inp": "tsx --inspect-brk --env-file=.env packages/test-interface-deep2/cli.ts generate 'glauth-master/v2/pkg/handler/ldap.go' -l golang -f testify -m 'GigaChat-Max'",
    "tests:console-deep2:inp": "tsx --inspect-brk --env-file=.env packages/test-interface-deep2/cli.ts generate 'consoleFull/packages/ui/components/*/index.ts' -l vue-typescript -f vitest -m 'deepseek/deepseek-chat' --ref 'consoleFull/packages/ui/components/**/*'",
    "tests-g:console": "tsx --env-file=.env packages/test-interface-gigachat/index.mts generate 'consoleFull/packages/ui/components/*/index.ts' --ref 'consoleFull/packages/ui/components/**/*' -r -l vue-typescript -f vitest --out-ext .test.ts -o consoleFull/packages/ui/tests --comment 'делай правильные импорты' --base 'consoleFull/packages/ui/' -m GigaChat-Max",
    "tests:ruby": "tsx --env-file=.env packages/test-interface/index.mts generate 'RubyExamples/**/*.rb' -r -l ruby -f rspec",
    "tests:py": "tsx --env-file=.env packages/test-interface/index.mts generate 'python-fastapi-example-main/src/**/*.py' -r -l python -f unittest",
    "app": "tsx --env-file=.env packages/app/index.ts -m deepseek/deepseek-chat",
    "app:inspect": "tsx --inspect --env-file=.env packages/app/index.ts",
    "question": "tsx --env-file=.env ./lib/question.mts -m o1-preview",
    "question:f": "tsx --env-file=.env ./lib/question.mts -m o1-preview -f QUESTION.txt",
    "transcrib": "tsx --env-file=.env ./lib/transcribation.mts",
    "start:c": "tsx --env-file=.env ./lib/index.mts -c",
    "build": "tsc",
    "qwe:publish": "npm publish --access public",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@types/cli-progress": "^3.11.6",
    "@types/node": "^22.10.1",
    "cli-progress": "^3.12.0",
    "commander": "^12.1.0",
    "ffmpeg-static": "^5.2.0",
    "ffprobe-static": "^3.1.0",
    "fluent-ffmpeg": "^2.1.3",
    "gigachat-node": "^2.1.4",
    "glob": "^11.0.0",
    "ignore": "^6.0.2",
    "inquirer": "^12.1.0",
    "install": "^0.13.0",
    "minimist": "^1.2.8",
    "npm": "^11.0.0",
    "openai": "^4.71.1",
    "p-limit": "^6.1.0",
    "picocolors": "^1.1.1",
    "prompts": "^2.4.2",
    "tiktoken": "^1.0.17",
    "tsx": "^4.19.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/ffprobe-static": "^2.0.3",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/minimist": "^1.2.5",
    "@types/prompts": "^2.4.9",
    "typescript": "^5.6.3"
  },
  "files": [
    "lib",
    "README.md",
    "package.json",
    "package-lock.json",
    "utils",
    "packages"
  ]
}
